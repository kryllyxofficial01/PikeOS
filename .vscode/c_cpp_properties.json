{
    "configurations": [
        {
            "name": "Watcom",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_cdecl="
            ],
            "compilerPath": "/usr/bin/watcom/binl/wcc",
            "cStandard": "c99",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-4",
                "-d3",
                "-s",
                "-wx",
                "-ms",
                "-zl",
                "-zq"
            ]
        }
    ],
    "version": 4
}